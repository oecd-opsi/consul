<% provide :title do %>
  <%= "#{t("oecd_representative.header.title")} - #{t("oecd_representative.menu.legislation")}" %> -
  <%= "#{@process.title} - #{t("oecd_representative.progress_bars.index.title")}" %>
<% end %>

<%= back_link_to oecd_representative_legislation_process_milestones_path(@progressable),
                 t("oecd_representative.legislation.processes.edit.back") %>

<h2><%= @process.title %></h2>

<%= render "oecd_representative/legislation/processes/subnav", process: @process, active: "milestones" %>
<%= render "oecd_representative/progress_bars/progress_bars", progressable: @process %>
